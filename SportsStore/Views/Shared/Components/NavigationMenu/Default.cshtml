@model IEnumerable<string>

<div>
    <a asp-action="Index"
        asp-controller="Home" asp-route-category="">Home</a>
    @foreach(string category in Model ?? Enumerable.Empty<string>())
    {
        <a class="@(category == ViewBag.SelectedCategory ? "awawa" : "owowo")"
            asp-action="Index"
            asp-controller="Home" asp-route-category="@category"
            asp-route-productPage="1">
            @category
        </a>
    }
</div>